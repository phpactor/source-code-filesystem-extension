{
    "name": "phpactor/source-code-filesystem-extension",
    "type": "phpactor-extension",
    "description": "Source Code Filesystem Extension",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Leech",
            "email": "daniel@dantleech.com"
        }
    ],
    "require": {
        "phpactor/composer-autoloader-extension": "^0.2.3",
        "phpactor/container": "^2.0.0",
        "phpactor/file-path-resolver-extension": "^0.3.4",
        "phpactor/source-code-filesystem": "^0.1.6",
        "php": "^7.3 || ^8.0"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.0",
        "friendsofphp/php-cs-fixer": "^2.17",
        "phpstan/phpstan": "~0.12.0",
        "phpunit/phpunit": "^9.0",
        "phpspec/prophecy-phpunit": "^2.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        },
        "phpactor.extension_class": "Phpactor\\Extension\\SourceCodeFilesystem\\SourceCodeFilesystemExtension"
    },
    "autoload": {
        "psr-4": {
            "Phpactor\\Extension\\SourceCodeFilesystem\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phpactor\\Extension\\SourceCodeFilesystem\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
